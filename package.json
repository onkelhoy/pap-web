{
  "name": "@papit/web-framework",
  "version": "1.0.10",
  "description": "Pap web framework",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/onkelhoy/web-components.git"
  },
  "scripts": {
    "create": "sh ./scripts/create/run.sh",
    "create:package": "sh ./scripts/create/run.sh --package",
    "create:sub": "sh ./scripts/create/run.sh --sub-package",
    "create:project": "sh ./scripts/create/run.sh --project",
    "clean": "sh ./scripts/_utils/clean.sh",
    "init": "sh ./scripts/_utils/init.sh"
  },
  "keywords": [
    "web-component",
    "typescript",
    "sass"
  ],
  "author": "Henry Pap",
  "license": "MIT",
  "workspaces": [
    "packages/**/*"
  ],
  "devDependencies": {
    "playwright": "^1.34.3",
    "node-html-parser": "^6.1.11"
  }
}